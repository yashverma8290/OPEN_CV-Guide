   "id": "7ce64763-46fc-4afd-96a8-996f884f2a09",
   "metadata": {},
   "outputs": [],
   "source": [
    "img=cv2.imread('image9.jpg')\n",
    "re_img=cv2.resize(img,(300,300))\n",
    "\n",
    "h=np.hstack((re_img,re_img))\n",
    "v=np.vstack((h,h))\n",
    "\n",
    "cv2.imwrite(\"newimg.jpg\",v)\n",
    "cv2.imshow('elon',v)\n",
    "cv2.waitKey(0)\n",
    "cv2.destroyAllWindows()"
   ]
